{
	"compilerOptions": {
		"target": "es5",
		"lib": [
		  "dom",
		  "dom.iterable",
		  "esnext"
		],
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
	},
	"include": ["src"],
}